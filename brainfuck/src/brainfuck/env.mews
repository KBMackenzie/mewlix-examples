yarn ball brainfuck.env

clowder Env
  =^.x.^= wake()
    home.cells = std.table()
    home.pointer = 0
  meowmeow

  =^.x.^= look()
    bring home.cells.get(home.pointer) or 0
  meowmeow

  =^.x.^= move(incr)
    home.pointer = home.pointer + incr
  meowmeow

  =^.x.^= set(value)
    home.cells.add(home.pointer, value % 256)
  meowmeow

  =^.x.^= modify(value)
    home.cells.add(home.pointer, (home.look() + value) % 256)
  meowmeow
meowmeow
